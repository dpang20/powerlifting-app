{
  "expo": {
    "name": "powerlifting-app",
    "slug": "powerlifting-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "dependencies": {
      "expo-router": "^1.0.0",
      "react": "18.0.0",
      "react-native": "0.70.0"
    },
    
    "plugins": [
      "expo-router"
    ]
  }
}
